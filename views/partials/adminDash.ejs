<h1>Admin Dashboard</h1>
  <h2>Hello, <%= username %>!</h2>
  <h3>Registered Users:</h3>
  <table border="2">
    <thead>
      <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% USERS.forEach((user) => { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>